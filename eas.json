{
  "cli": {
    "version": ">= 12.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_DATABASE_URL": "EXPO_PUBLIC_DATABASE_URL",
        "EXPO_PUBLIC_AUTH_BASE_URL": "EXPO_PUBLIC_AUTH_BASE_URL",
        "EXPO_PUBLIC_GOOGLE_CLIENT_ID": "EXPO_PUBLIC_GOOGLE_CLIENT_ID",
        "EXPO_PUBLIC_GOOGLE_CLIENT_SECRET": "EXPO_PUBLIC_GOOGLE_CLIENT_SECRET",
        "EXPO_PUBLIC_DEV_MODE": "true",
        "EXPO_PUBLIC_ADMIN_EMAIL": "EXPO_PUBLIC_ADMIN_EMAIL",
        "EXPO_PUBLIC_R2_ACCOUNT_ID": "EXPO_PUBLIC_R2_ACCOUNT_ID",
        "EXPO_PUBLIC_R2_ACCESS_KEY_ID": "EXPO_PUBLIC_R2_ACCESS_KEY_ID",
        "EXPO_PUBLIC_R2_SECRET_ACCESS_KEY": "EXPO_PUBLIC_R2_SECRET_ACCESS_KEY",
        "EXPO_PUBLIC_R2_BUCKET_NAME": "EXPO_PUBLIC_R2_BUCKET_NAME",
        "EXPO_PUBLIC_R2_ENDPOINT": "EXPO_PUBLIC_R2_ENDPOINT",
        "EXPO_PUBLIC_R2_PUBLIC_URL": "EXPO_PUBLIC_R2_PUBLIC_URL"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_DATABASE_URL": "EXPO_PUBLIC_DATABASE_URL",
        "EXPO_PUBLIC_AUTH_BASE_URL": "EXPO_PUBLIC_AUTH_BASE_URL",
        "EXPO_PUBLIC_GOOGLE_CLIENT_ID": "EXPO_PUBLIC_GOOGLE_CLIENT_ID",
        "EXPO_PUBLIC_GOOGLE_CLIENT_SECRET": "EXPO_PUBLIC_GOOGLE_CLIENT_SECRET",
        "EXPO_PUBLIC_DEV_MODE": "false",
        "EXPO_PUBLIC_ADMIN_EMAIL": "EXPO_PUBLIC_ADMIN_EMAIL",
        "EXPO_PUBLIC_R2_ACCOUNT_ID": "EXPO_PUBLIC_R2_ACCOUNT_ID",
        "EXPO_PUBLIC_R2_ACCESS_KEY_ID": "EXPO_PUBLIC_R2_ACCESS_KEY_ID",
        "EXPO_PUBLIC_R2_SECRET_ACCESS_KEY": "EXPO_PUBLIC_R2_SECRET_ACCESS_KEY",
        "EXPO_PUBLIC_R2_BUCKET_NAME": "EXPO_PUBLIC_R2_BUCKET_NAME",
        "EXPO_PUBLIC_R2_ENDPOINT": "EXPO_PUBLIC_R2_ENDPOINT",
        "EXPO_PUBLIC_R2_PUBLIC_URL": "EXPO_PUBLIC_R2_PUBLIC_URL"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "cache": {
        "disabled": false
      },
      "env": {
        "EXPO_PUBLIC_DATABASE_URL": "EXPO_PUBLIC_DATABASE_URL",
        "EXPO_PUBLIC_AUTH_BASE_URL": "EXPO_PUBLIC_AUTH_BASE_URL",
        "EXPO_PUBLIC_GOOGLE_CLIENT_ID": "EXPO_PUBLIC_GOOGLE_CLIENT_ID",
        "EXPO_PUBLIC_GOOGLE_CLIENT_SECRET": "EXPO_PUBLIC_GOOGLE_CLIENT_SECRET",
        "EXPO_PUBLIC_DEV_MODE": "false",
        "EXPO_PUBLIC_ADMIN_EMAIL": "EXPO_PUBLIC_ADMIN_EMAIL",
        "EXPO_PUBLIC_R2_ACCOUNT_ID": "EXPO_PUBLIC_R2_ACCOUNT_ID",
        "EXPO_PUBLIC_R2_ACCESS_KEY_ID": "EXPO_PUBLIC_R2_ACCESS_KEY_ID",
        "EXPO_PUBLIC_R2_SECRET_ACCESS_KEY": "EXPO_PUBLIC_R2_SECRET_ACCESS_KEY",
        "EXPO_PUBLIC_R2_BUCKET_NAME": "EXPO_PUBLIC_R2_BUCKET_NAME",
        "EXPO_PUBLIC_R2_ENDPOINT": "EXPO_PUBLIC_R2_ENDPOINT",
        "EXPO_PUBLIC_R2_PUBLIC_URL": "EXPO_PUBLIC_R2_PUBLIC_URL"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./google-service-account.json",
        "track": "internal"
      }
    }
  }
}
