{
  "functions": {
    "api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/auth",
      "destination": "/api/auth/index"
    },
    {
      "source": "/api/auth/(.*)",
      "destination": "/api/auth/index"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "buildCommand": "npm install",
  "outputDirectory": ".",
  "installCommand": "npm install"
}
